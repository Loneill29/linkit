<h1>All Questions</h1>

<div class="row">
  <div class="col-md-8">
    <% @questions.each do |question| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to question.title %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
  <div class="col-md-4">
    <%= link_to "Ask a Question", new_question_path, class: 'btn btn-success' %>
  </div>
</div>
