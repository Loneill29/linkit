<div class="row">
  <div class="col-md-8">
    <br>
    <h1 class="linkit">Questions
    <%= link_to "Ask a Question", new_question_path, class: 'btn btn-primary question' %></h1>
    <% @questions.each do |question| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to question.title, question %>
          </h4>
        </div>
      </div>
      <hr>
    <% end %>
  </div>
  <div class="col-md-4">
  </div>
</div>
